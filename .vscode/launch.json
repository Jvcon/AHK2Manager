    "version": "0.2.0",
    "configurations": [
        {
            "name": "Normal Debug",
            "type": "autohotkey",
            "request": "launch",
            "program": "${file}",
            "args": [],
            "port": "9002-9010",
            "variableCategories": "recommend",
            "useDebugDirective": true,
            "useAutoJumpToError": true
        },
        {
            "name": "SwitchRC Mode",
            "type": "autohotkey",
            "request": "launch",
            "program": "${file}",
            "args": ["mode","rc"],
            "port": "9002-9010",
            "variableCategories": "recommend",
            "useDebugDirective": true,
            "useAutoJumpToError": true
        },
       
    ]
}